<?xml version="1.0"?>
<odoo>
    <record id="sale_order_general_lead_time_form_view" model="ir.ui.view">
        <field name="name">sale.order.general.lead.time.form</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="delivery.view_order_form_with_carrier"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='payment_term_id']" position="after">
                <field name="general_lead_time"/>
            </xpath>
        </field>
    </record>

</odoo>
