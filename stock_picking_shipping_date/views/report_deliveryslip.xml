<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="report_delivery_document" inherit_id="stock_views_extension.report_delivery_document">
        <xpath expr="//span[@t-field='o.company_id']/.." position="before">
            <div class="row" t-if="o.shipping_date">
                <span>Shipping Date: </span>
                <span t-field="o.shipping_date" class="m-0"/>
            </div>
        </xpath>
    </template>
</odoo>
